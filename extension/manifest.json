{
  "manifest_version": 3,
  "name": "Codefolio",
  "version": "1.7.0",
  "description": "Automatically update your resume with your GitHub projects.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}